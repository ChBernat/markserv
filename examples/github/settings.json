{
  "name": "Github Documentation",

  "root": ".",

  "map": {

    "directory": {
      "plugin": "dir",
      "template": "directory-listing.html"
    },

    "markdown": {
      "plugin": "markdownToHtml",
      "template": "markdown-document.html"
    },

    "404": {
      "plugin": "404",
      "template": "404.html"
    },

    "watch": [
      ".less",
      ".js",
      ".css",
      ".html",
      ".htm",
      ".json",
      ".gif",
      ".png",
      ".jpg",
      ".jpeg"
    ]

  }
}
